{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "codeat3/blade-phosphor-icons": "^2.3",
        "filament/filament": "^4.0",
        "laravel/breeze": "^2.3.7",
        "laravel/framework": "^12.20.0",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.6.3",
        "sentry/sentry-laravel": "^4.15.1",
        "spatie/laravel-markdown": "^2.7.1",
        "aws/aws-sdk-php": "^3.349.3",
        "league/flysystem-aws-s3-v3": "^3.29.0",
        "livewire/flux": "^2.2.2",
        "livewire/flux-pro": "^2.2.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15.4",
        "driftingly/rector-laravel": "^2.0.5",
        "fakerphp/faker": "^1.24.1",
        "filament/upgrade": "^4.0",
        "laravel/pint": "^1.23.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "pestphp/pest": "^3.8.2",
        "pestphp/pest-plugin-faker": "^v3.0.0",
        "pestphp/pest-plugin-laravel": "^3.2.0",
        "pestphp/pest-plugin-type-coverage": "^3.6.0",
        "roave/security-advisories": "dev-latest",
        "rector/rector": "^2.1.0",
        "larastan/larastan": "^3.5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "test:lint": [
            "pint --test",
            "@test:refactor"
        ],
        "refactor": "rector",
        "test:refactor": "rector --dry-run",
        "test:types": "phpstan analyse",
        "test:arch": "pest --filter=arch",
        "test:type-cov": "pest --type-coverage --min=100",
        "test:unit": "XDEBUG_MODE=coverage php ./vendor/bin/pest --parallel --coverage",
        "lint": [
            "pint",
            "@refactor"
        ],
        "test": [
            "@test:lint",
            "@test:types",
            "@test:arch",
            "@test:unit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "repositories": {
        "flux-pro": {
            "type": "composer",
            "url": "https://composer.fluxui.dev"
        }
    }
}
